package exercise4;

import java.util.ArrayList;

public class TestSearching {

	public static void main(String[] args) {

		ArrayList<Player> players = new ArrayList<>();
		Player p1 = new Player("Jonas", 180, 64, 23);
		Player p2 = new Player("Kurt", 160, 58, 17);
		Player p3 = new Player("Noob", 200, 100, 0);
		Player p4 = new Player("Henrik", 188, 80, 25);
		Player p5 = new Player("Per", 192, 82, 21);
		Player p6 = new Player("Mads", 182, 76, 25);
		players.add(p1);
		players.add(p2);
		players.add(p3);
		players.add(p4);
		players.add(p5);
		players.add(p6);
		
		p1.findPlayerLinear(players, 25);

	}

}
