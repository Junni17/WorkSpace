package team_and_player;

import java.util.ArrayList;

public class Team {

	private String name;
	private ArrayList<Player> players;

	public Team(String name) {
		this.name = name;
		this.players = new ArrayList<Player>();
	}

	public String getName() {
		return this.name;
	}
	
	public ArrayList getPlayers(){
		return this.players;
	}
}
