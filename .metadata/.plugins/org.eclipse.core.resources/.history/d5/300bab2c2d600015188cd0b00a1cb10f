package pig;

public class PigPlayer 
{
	private Die die1;
	private int point;
	private int sum;
	private boolean win;
	private boolean lose;
	
	public PigPlayer()
	{
		this.die1 = new Die();
		this.point = 0;
		this.sum = 0;
		win = false;
		lose = false;
	}
	
	
	public int getPoint()
	{
		return point;
	}
	
	public int getSum()
	{
		return sum;
	}
	
	

}
