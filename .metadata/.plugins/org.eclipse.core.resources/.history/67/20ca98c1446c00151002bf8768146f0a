package exercise5;

public class SequenceTest {

	public static void main(String[] args) {

		Sequence s1 = new Sequence();

		int i = 1;
		while (i < 30) {
			s1.add(i);
			i += 2 * i;
		}

		Sequence s2 = new Sequence();
		s2.add(10);
		s2.add(3);
		s2.add(4);
		s2.add(16);
		s2.add(6);

		System.out.println(s1.toString());
		System.out.println(s2.toString());

	}

}
