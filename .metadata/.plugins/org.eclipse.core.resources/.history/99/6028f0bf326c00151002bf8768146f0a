package exercise3;

import java.util.Arrays;

public class SequenceTest {

	public static void main(String[] args) {

		Sequence s1 = new Sequence(7);
		s1.set(0, 5);
		s1.set(1, 2);
		s1.set(2, 2);
		s1.set(3, 2);
		s1.set(4, 4);
		s1.set(5, 5);
		s1.set(6, 4);

		Sequence s2 = new Sequence(6);
		s2.set(0, 2);
		s2.set(1, 2);
		s2.set(2, 5);
		s2.set(3, 4);
		s2.set(4, 4);
		s2.set(5, 5);

		System.out.println(s1.sameValues(s2));
		System.out.println("Smallest: " + Arrays.toString(s1.smallest(s2)));

		System.out.println("Biggest: " + Arrays.toString(s1.biggest(s2)));
	}

}
